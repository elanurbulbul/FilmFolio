{
   "version": 2,
   "builds": [
     {
       "src": "src/index.html",
       "use": "@vercel/static-build"
     }
   ],
   "routes": [
     { "src": "/[^.]+", "dest": "/", "status": 200 }
   ],
   "env": {
     "VITE_API_BASE_URL": "@VITE_API_BASE_URL",
     "VITE_API_KEY": "@VITE_API_KEY",
     "VITE_API_TOKEN": "@VITE_API_TOKEN"
   }
 }
 